{
  "name": "superfriend",
  "version": "0.0.1",
  "description": "Where we stalk your friends",
  "main": "index.js",
  "scripts": {
    "validate": "check-node-version --node '>= 6.7.0'",
    "setup": "./bin/setup",
    "prep": "npm run validate && npm run setup",
    "postinstall": "npm run prep",
    "build-frontend": "webpack",
    "build-frontend-watch": "npm run build-frontend -- -w",
    "build-css": "node-sass public/stylesheets/style.scss -o public/stylesheets",
    "build-css-watch": "npm run build-css -- -w",
    "build": "concurrently \"npm run build-frontend\" \"npm run build-css\"",
    "build-watch": "concurrently --kill-others \"npm run build-frontend-watch\" \"npm run build-css-watch\"",
    "build-dev": "cross-env NODE_ENV=development npm run build-watch",
    "build-branch": "bin/build-branch.sh",
    "start": "node server/start.js",
    "start-watch": "nodemon server/start.js --watch server --watch db",
    "start-dev": "cross-env NODE_ENV=development npm run start-watch",
    "lint": "esw --ext .js,.jsx --ignore-path .gitignore --cache --format node_modules/eslint-formatter-pretty",
    "lint-watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "npm run test -- --watch",
    "test-dev": "npm run test-watch -- -R mocha-tldr-reporter",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others --prefix \"[{name}]\" --names \"BUILD,SERVE,LINT ,TEST \" -c \"bgBlue.black,bgGreen.black,bgYellow.black,bgMagenta.black\" \"npm run build-dev\" \"npm run start-dev\" \"npm run lint-watch\" \"npm run test-dev\"",
    "seed": "node db/seed",
    "deploy-heroku": "bin/deploy-heroku.sh",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queerviolet/bones.git"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Ashi Krishnan <me@ashi.works>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/queerviolet/bones/issues"
  },
  "homepage": "https://github.com/queerviolet/bones#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-preset-stage-2": "^6.18.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "concurrently": "^3.4.0",
    "cookie-session": "^2.0.0-alpha.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "express-session": "^1.15.1",
    "finalhandler": "^1.0.0",
    "google-auth-library": "^0.10.0",
    "googleapis": "^18.0.0",
    "mailparser": "^2.0.2",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "node-gmail-api": "^0.6.0",
    "node-gyp": "^3.6.0",
    "node-pre-gyp": "^0.6.34",
    "node-sass": "^4.5.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pretty-error": "^2.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.81.0",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "6",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.16.1",
    "eslint-config-fullstack": "^2.7.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "husky": "^0.13.1",
    "mocha": "^3.2.0",
    "mocha-tldr-reporter": "^1.0.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "volleyball": "^1.4.1",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0"
  }
}
